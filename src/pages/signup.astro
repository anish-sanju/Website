---
import SignupForm from "../components/forms/Signup.astro";
import BaseLayout from "../layouts/BaseLayout.astro";

---
<BaseLayout
 page='Sign Up'
 description='Sign Up page for meals on wheels'
>
  <SignupForm
    title='Welcome!'
    desc='Enter your details to signup.' />
</BaseLayout>
<script>
  const isLoggedIn = localStorage.getItem('isLoggedIn');
  if (isLoggedIn !== null)
      localStorage.clear()
</script>